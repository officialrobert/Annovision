    "build": {
        "asar": false,
        "files": [
            "core/**/*",
            "app.config",
            "pysc/**/*",
            "pycorewin/**/*"
        ],
        "win": {
            "icon": "core/app.ico"
        }
    },
    "babel": {
        "presets": [
            "react-app",
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ]
        ]
    }
}
